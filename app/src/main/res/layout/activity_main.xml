<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/touchArea"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.wavemaker.MainActivity">

    <Button
        android:id="@+id/lockButton"
        android:layout_width="81dp"
        android:layout_height="44dp"
        android:layout_marginStart="164dp"
        android:layout_marginTop="60dp"
        android:text="Lock"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/modFrequency"
        android:layout_width="46dp"
        android:layout_height="39dp"
        android:layout_marginStart="160dp"
        android:layout_marginTop="394dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="440"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="40dp"
        android:layout_height="39dp"
        android:layout_marginStart="206dp"
        android:layout_marginTop="394dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Hz"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/modBar"
        android:layout_width="206dp"
        android:layout_height="94dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="300dp"
        android:max="1000"
        android:min="200"
        android:progress="440"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/carBar"
        android:layout_width="206dp"
        android:layout_height="94dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="524dp"
        android:max="1000"
        android:min="200"
        android:progress="440"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/startButton"
        android:layout_width="81dp"
        android:layout_height="44dp"
        android:layout_marginStart="164dp"
        android:layout_marginTop="116dp"
        android:text="Play"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/carrierFrequency"
        android:layout_width="46dp"
        android:layout_height="39dp"
        android:layout_marginStart="160dp"
        android:layout_marginTop="618dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="440"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="40dp"
        android:layout_height="39dp"
        android:layout_marginStart="206dp"
        android:layout_marginTop="618dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Hz"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/modtextfreq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="136dp"
        android:layout_marginTop="236dp"
        android:text="Modulator Frequency"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/carFreq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="148dp"
        android:layout_marginTop="480dp"
        android:text="Carrier Frequency"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>

